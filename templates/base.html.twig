<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="icon" href="{{asset('/img/custom/logos/default/logo-default.png')}}" id="light-scheme-icon" sizes="32x32"> 
		<link rel="icon" href="{{asset('/img/custom/logos/default/logo-default.png')}}" id="light-scheme-icon" sizes="32x32"> 
	
		<title>
			{% block title %}CELL Form{% endblock %}
		</title>
	
		{% block stylesheets %}
			{# <link href="{{ asset('/css/style.css') }}" rel="stylesheet"/> #}

			{{ encore_entry_link_tags('app') }}
			{# {% if app.user == false and app.request.get('_route') != 'questionnaire' %}
				<style>
					.color_theme {
						background: #000000 !important;
						border-color: #363636;
					} 
					.color_text_theme {
						color: white;
					}
				</style>
			{% endif %} #}
			{# {% if app.user and app.user.personalization != null or app.request.get('_route') == 'questionnaire'  %} #}
