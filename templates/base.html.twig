				<style>
					.color_theme {
						background: {{ app.user ? app.user.personalization.color : personalization.color}} !important;
					}
					.color_text_theme {
						color: white;
					}
				</style>
			{% endif %}

		{% endblock %}

	</head>
	<body>

		<div class="clearfix">
			{% include 'partials/header.html.twig' %}

			<div class="min_heigth">
				{% block body %}{% endblock %}
			</div> 
			

			{% include 'partials/footer.html.twig' %}

			{% block javascripts %}
				{{ encore_entry_script_tags('app') }}
			{% endblock %}
		</div>
	</body>
</html>

